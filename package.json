    "start:watch": "nodemon",
    "build": "tsc"
  },
  "author": "Nicolas Bruegger <nicolas.bruegger@tingo>",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/ip": "^1.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ip": "^1.1.5"
  },
  "devDependencies": {
    "@types/node": "^12.12.7",
    "eslint": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.4",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
